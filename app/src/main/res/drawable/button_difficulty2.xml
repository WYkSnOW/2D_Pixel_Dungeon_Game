<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="#B29800" />
            <gradient
                android:angle="0"
                android:startColor="#B2FFFB00"
                android:centerColor="#B2FFFB00"
                android:endColor="#B2FFFB00"
                android:type="linear"/>
            <corners android:radius="30dp" />
            <stroke
                android:width="1dp"
                android:color="#FFEB3B"/>
        </shape>
    </item>
</selector>